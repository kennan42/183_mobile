					</xsd:extension>
				</xsd:complexContent>
			</xsd:complexType>

			<xsd:complexType name="ZSD_B2B_013_T">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="0" name="MATNR" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>产品ID</xsd:documentation>
