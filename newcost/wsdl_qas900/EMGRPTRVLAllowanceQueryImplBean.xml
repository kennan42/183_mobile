    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="EM_GRPTRVL_AllowanceQuery">
    <wsdl:port binding="tns:EM_GRPTRVL_AllowanceQuerySOAP" name="EM_GRPTRVL_AllowanceQuerySOAP">
