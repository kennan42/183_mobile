        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="AllowanceQueryOutput">
        <xsd:complexContent>
          <xsd:extension base="tns:ReturnCode">
            <xsd:sequence>
              <xsd:element maxOccurs="unbounded" minOccurs="0" name="travelsGrpAllowanceList" type="tns:TravelsGrpAllowance">
