      </xsd:complexType>
      <xsd:complexType name="TravelsGrp">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="travelsList" type="tns:Travels"/>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="TravelsGrpAllowance">
        <xsd:sequence>
